{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "lazy",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      },
      "cacheConfig": {
        "maxSize": 2,
        "maxAge": "30d",
        "strategy": "freshness",
        "timeout": "0u"
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      },
      "cacheConfig": {
        "maxSize": 2,
        "maxAge": "30d",
        "strategy": "performance",
        "timeout": "2s"
      }
    },
    {
      "name": "api",
      "resources": {
        "urls": [
          "https://www.boredapi.com/api/activity",
          "https://catfact.ninja/fact"
        ]
      },
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "2d",
        "strategy": "freshness",
        "timeout": "0u"
      }
    }
  ]
}